{
  "version": 2,
  "name": "ai-diagnostic-webapp",
  "builds": [
    {
      "src": "backend/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/public/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/public/index.html"
    }
  ],
  "env": {
    "PYTHONPATH": "backend"
  }
}
