{
  "version": 2,
  "builds": [
    {
      "src": "frontend/public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(css|js|ico|png|jpg|jpeg|gif|svg))",
      "dest": "/frontend/public/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/public/index.html"
    }
  ]
}
